    "@types/node": "^22.14.1",
    "@types/pg": "^8.11.11",
    "@unocss/nuxt": "^66.0.0",
    "@unocss/preset-wind": "^66.0.0",
    "@unocss/reset": "^66.0.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/nuxt": "^13.1.0",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.25.1",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.5.1",
    "nitropack": "^2.11.9",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "shipjs": "^0.27.0",
    "stylelint": "^16.19.0",
    "stylelint-config-recommended-vue": "^1.6.0",
