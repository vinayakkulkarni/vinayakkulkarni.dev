{
  "name": "vinayakkulkarni.dev",
  "version": "1.0.0",
  "description": "Vinayak Portfolio using amazing Nuxt.js",
  "author": "Vinayak Kulkarni",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "netlify": "netlify dev",
    "build": "nuxt generate",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:eslint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix:eslint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt)/**/}*.{js,vue}\" --ignore-path .gitignore",
    "lintfix:prettier": "prettier --write \"{,!(node_modules|.nuxt)/**/}*.{js,vue}\" --ignore-path .gitignore",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lintfix:style": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:style",
    "lintfix": "npm run lintfix:eslint && npm run lintfix:prettier && npm run lintfix:style",
    "test": "ava"
  },
  "dependencies": {
    "@nuxt/content": "^1.9.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.1.0",
    "@tailwindcss/typography": "^0.2.0",
    "@tailwindcss/ui": "^0.6.2",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.14.6",
    "prism-themes": "^1.4.1",
    "vuescroll": "^4.16.1"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/test-utils": "^1.1.0",
    "ava": "^3.13.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-env": "^3.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "netlify-cli": "^2.65.5",
    "prettier": "^2.1.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0"
  }
}
