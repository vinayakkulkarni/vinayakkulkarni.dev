{
  "name": "vinayakkulkarni.dev",
  "description": "Vinayak's personal website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "is-ci || husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "source .drizzle.env && drizzle-kit push",
    "db:studio": "source .drizzle.env && drizzle-kit studio",
    "lint": "bun run lint:prettier && bun run lint:eslint && bun run lint:stylelint",
    "lintfix": "bun run lint:prettier:fix && bun run lint:eslint:fix && bun run lint:stylelint:fix",
    "lint:js": "bun run lint:prettier && bun run lint:eslint",
    "lint:eslint": "eslint \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-pattern .gitignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-pattern .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:stylelint": "stylelint '**/*.{css,scss,sass,vue}' --ignore-path .gitignore",
    "lint:stylelint:fix": "stylelint --fix '**/*.{css,scss,sass,vue}' --ignore-path .gitignore",
    "release": "shipjs prepare",
    "release:dry": "shipjs prepare --dry-run",
    "release:auto": "shipjs prepare --yes"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@nuxt/icon": "^1.5.1",
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/fontaine": "^0.4.3",
    "@nuxtjs/html-validator": "^1.8.2",
    "arctic": "^1.9.2",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "lucia": "3.2.0",
    "nuxt": "^3.13.2",
    "nuxt-time": "^0.1.3",
    "oslo": "^1.2.1",
    "postgres": "^3.4.4",
    "vue": "^3.5.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@awmottaz/prettier-plugin-void-html": "^1.6.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@nuxt/content": "^2.13.2",
    "@nuxt/devtools": "^1.5.0",
    "@nuxt/eslint": "^0.5.7",
    "@nuxtjs/plausible": "^1.0.2",
    "@types/bun": "^1.1.9",
    "@types/node": "^22.5.5",
    "@unocss/nuxt": "^0.62.4",
    "@unocss/preset-wind": "^0.62.4",
    "@unocss/reset": "^0.62.4",
    "@vueuse/core": "^11.1.0",
    "@vueuse/nuxt": "^11.1.0",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.10.0",
    "husky": "^9.1.6",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.10",
    "nitropack": "^2.9.7",
    "prettier": "^3.3.3",
    "sass": "^1.79.1",
    "shipjs": "^0.26.4",
    "stylelint": "^16.9.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.6.2",
    "unocss-preset-scrollbar": "^0.3.1"
  },
  "packageManager": "bun@1.1.27",
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinayakkulkarni/vinayakkulkarni.dev.git"
  },
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/vinayakkulkarni.dev/issues"
  },
  "homepage": "https://vinayakkulkarni.dev/"
}
