{
  "name": "vinayakkulkarni.dev",
  "version": "0.1.0",
  "description": "Vinayak's personal website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:css",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix && npm run lint:css:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.{css,scss,sass,vue}' --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix '**/*.{css,scss,sass,vue}' --ignore-path .gitignore",
    "release": "shipjs prepare",
    "release:dry": "shipjs prepare --dry-run",
    "release:auto": "shipjs prepare --yes",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@kevinmarrec/nuxt-pwa": "^0.17.0",
    "@nuxt/content": "npm:@nuxt/content-edge@latest",
    "@nuxtjs/plausible": "^0.2.1",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@unocss/eslint-config": "^0.54.2",
    "@unocss/nuxt": "^0.54.2",
    "@unocss/preset-wind": "^0.55.0",
    "@unocss/reset": "^0.54.1",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/runtime-dom": "^3.3.4",
    "@vueuse/core": "^10.2.1",
    "@vueuse/nuxt": "^10.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nuxt": "^3.6.5",
    "prettier": "^3.0.1",
    "sass": "^1.64.2",
    "shipjs": "^0.26.3",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-prettier": "^4.0.2",
    "typescript": "^5.1.6",
    "unocss-preset-scrollbar": "^0.2.1"
  },
  "repository": {
    "url": "git+https://github.com/vinayakkulkarni/vinayakkulkarni.dev.git",
    "type": "git"
  },
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/vinayakkulkarni.dev/issues"
  },
  "homepage": "https://vinayakkulkarni.dev/"
}
